import java.util.*;
class treemapp
{
    public static void main(String args[])
    {
       
        TreeMap<String,String> student =new TreeMap<String,String> ();
        student.put("a1","ram");
         student.put("a2","shyam");
          student.put("a3","samir");
           student.put("a4","virat");
            System.out.println("hashmap contains"+student);
            //removing all elemnets
              student.clear();
        
              System.out.println("after removing all elements:");
              System.out.println("hashmap contains:"+student);
    }
}

